
public class RotateImage {
	public void rotate(int[][] matrix) {
        int n = matrix.length;
        int[][] rotate = new int[n][n];
        
        for (int i = 0; i < n; i++){
        	for (int j = 0; j < n;j++){
        		rotate[j][i] = matrix[i][n-j-1];
        	}
        }
        
        for (int i = 0; i < n; i++){
        	for (int j = 0; j < n;j++){
        		matrix[i][j] = rotate[i][j];
        	}
        }
    }
}
