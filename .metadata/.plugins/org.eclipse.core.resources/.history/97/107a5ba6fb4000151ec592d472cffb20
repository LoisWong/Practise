
public class spiralArray {
	public void main(String[] args){
		int[][] spiral = generateMatrix(3);
		for(int i = 0; i < 3; i++){
			for(int j = 0; j < 3; j++){
				System.out.print(spiral[i][j]);
			}
			System.out.println();
		}
	}
	public int[][] generateMatrix(int n) {
        int[][] Matrix = new int[n][n];
        int count = 0;
        while (count <= n*n){
            
        }
        return Matrix;
    }
}
